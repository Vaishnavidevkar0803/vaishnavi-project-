<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="listeners.Listner"/><!-- packageName.clasName--> 
	</listeners>
  <test thread-count="5" name="Test">
    <classes>
      <class name="testNG_Study.MyClass"/>
      <class name="testNG_Study.MyClass2"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
<!-- this is how u can add listner at suit level and run 2 classes at a time-->
